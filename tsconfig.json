{
  "compilerOptions": {
    "jsx": "react-jsx",
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "ESNext",
    "moduleResolution": "node",
    "outDir": "dist",
    "target": "ESNext",
    "sourceMap": true,
    "baseUrl": "src",
    "paths": { 
      "@/*": ["*"],
      "assets/*":["./assets"]
    },
    "typeRoots": ["./src/@types", "./node_modules/@types"]
  },
  "include": ["src"]
}
